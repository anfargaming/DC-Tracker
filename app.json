{
    "name": "DC Tracker Bot",
    "description": "A bot to find telegram dc id",
    "repository": "https://github.com/anfargaming/DC-Tracker",
    "keywords": [
        "telegram",
        "user",
        "dc_id",
        "data_center",
        "finder",
        "pyrogram"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        },
        "UPDATE_CHANNEL": {
            "description": "Your Channel Username Without @",
            "value": "BX_Botz"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
